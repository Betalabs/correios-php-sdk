{
    "name": "betalabs/correios-php-sdk",
    "description": "SDK para integração com APIs dos Correios.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "betalabs",
            "email": "betalabs@betalabs.com.br"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
       "guzzlehttp/guzzle": "^6.3 || ^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CorreiosPhpSdk\\": "src/CorreiosPhpSdk/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CorreiosPhpSdk\\Tests\\": "tests/CorreiosPhpSdk/"
        }
    },
    "minimum-stability": "dev"
}
